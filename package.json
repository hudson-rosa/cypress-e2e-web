{
  "name": "cypress-e2e-web",
  "version": "1.0.0",
  "description": "cypress-sample",
  "main": "index.js",
  "author": "hudson.rosa",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "npx cypress open --browser chrome",
    "cy:run": "TEST_ENV=dev npx cypress run --browser chrome",
    "cy:run:headed:spec": "TEST_ENV=dev TZ=UTC npx cypress run --browser chrome --headed --spec",
    "cy:run:headed:tag": "TEST_ENV=dev TZ=UTC npx cypress run --browser chrome --headed tags=",
    "cy:run:headed:all": "TEST_ENV=dev TZ=UTC npx cypress run --browser chrome --headed tags='@regression'"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@badeball/cypress-cucumber-preprocessor": "^15.1.4",
    "cypress": "^12.7.0",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": [
      "cypress/e2e/steps/**/*.steps.{js,ts}",
      "cypress/support/step_definitions/**/*.{js,ts}"
    ]
  }
}
